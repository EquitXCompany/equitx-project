{
  "devDependencies": {
    "@astrojs/check": "^0.4.1",
    "@subql/cli": "^5.4.0",
    "@subql/types": "^3.11.4",
    "astro": "^4.2.4",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.1",
    "glob": "^10.4.1",
    "typescript": "^5.3.3"
  },
  "name": "equitx",
  "scripts": {
    "astro": "astro",
    "build": "LOAM_ENV=staging loam build --build-clients && astro check && astro build",
    "dev": "concurrently \"loam dev --build-clients\" \"astro dev\"",
    "preview": "astro preview",
    "start": "concurrently \"loam dev --build-clients\" \"astro dev\"",
    "subql:codegen": "cd subquery && subql codegen",
    "subql:build": "cd subquery && subql build",
    "subql:start": "cd subquery && docker-compose up"
  },
  "type": "module",
  "version": "0.0.1",
  "workspaces": [
    "packages/*",
    "subquery"
  ],
  "dependencies": {
    "@astrojs/react": "^3.6.2",
    "@emotion/react": "^11.13.3",
