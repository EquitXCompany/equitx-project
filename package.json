    "@mui/material": "^6.1.1",
    "@stellar/freighter-api": "^3.0.0",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "axios": "^1.7.9",
    "bignumber.js": "^9.1.2",
    "modern-normalize": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.26.2"
  }
